}

@media (max-width: 575px) {
    .nav-item {
        border-bottom-width: 1px;
        border-bottom-style: solid;
        border-bottom-color: #eaeaea;
        margin-left:20px;
        margin-right:20px;
    }

    .nav-item-last {
        border-bottom-color: #eaeaea00;
    }

    .navbar-nav {
        margin-top: 5px;
        margin-bottom: 20px;
    }

    @keyframes slideInRight {
        from {
            opacity: 0;
        }
        to {
            opacity: 1;
        }
    }

    @keyframes slideInRightFar {
        from {
            opacity: 0;
        }
        to {
            opacity: 1;
        }
    }

    @keyframes slideInLeftFar {
        from {
            opacity: 0;
        }
        to {
            opacity: 1;
        }
    }

    @keyframes slideInLeft {
        from {
            opacity: 0;
        }
        to {
            opacity: 1;
        }
    }

    @keyframes fadeIn {
        from {
            opacity: 0;
        }
        to {
            opacity: 1;
        }
    }

    .flip {
        -webkit-transform: rotate(180deg);
        -ms-transform: rotate(180deg);
        -moz-transform: rotate(180deg);
        -o-transform: rotate(180deg);
        transform: rotate(180deg);
    }

    .show {
        bottom: 17px;
    }
}

@media (min-width: 992px) {
    .sticky-cont-mg {
        margin-top: 200px;
        margin-bottom: 50px;
    }
    .sticky-elem {
        position: sticky;
        top: 40%;
        margin-left: 30px;
    }
}


    .zoom-bounds:hover .hidden-abs-text {
