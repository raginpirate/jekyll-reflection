@keyframes fadeInBackground {
    from {
        opacity: 0;
    }
    to {
        opacity: 1;
    }
}

@keyframes slideInRight {
    from {
        width: 120%;
        opacity: 0;
    }
    to {
        width: 100%;
        opacity: 1;
    }
}

@keyframes slideInRightFar {
    from {
        opacity: 0;
        width: 150%;
    }
    to {
        opacity: 1;
        width: 100%;
    }
}

@keyframes slideInLeftFar {
    from {
        opacity: 0;
        width: 50%;
    }
    to {
        opacity: 1;
        width: 100%;
    }
}

@keyframes slideInLeft {
    from {
        width: 80%;
        opacity: 0;
    }
    to {
        width: 100%;
        opacity: 1;
    }
}

@keyframes fadeIn {
    from {
        opacity: 0;
    }
    to {
        opacity: 1;
    }
}

.home-button {
    position: fixed;
    bottom: -40px;
    right: 17px;
    height: 29px;
    width: 29px;
    border-radius: 50%;
    z-index: 2;
    background: rgba(81, 81, 81, 0.5);
    outline:none !important;
    cursor: pointer;
    align-items: center;
    display: flex;
    border: 2px solid rgba(81, 81, 81, 0.5);
}

@media (min-width: 1340px) {
    .project-title {
