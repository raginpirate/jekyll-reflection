.d-block-hideable {
    display: block;
}

.background-wrapper {
    background-position: center;
    display: flex;
    flex-direction: column;
    justify-content: center;
    margin: auto;
}

.navbar {
    padding-top: 4px !important;
    padding-bottom: 4px !important;
    box-shadow: 1px 1px rgba(0, 0, 0, 0.1);
}

.navbar-light .navbar-nav .nav-link {
    color: #333;
}

.navbar-light .navbar-brand {
    color: #000;
    margin-right: 0px;
    min-width: 0px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.navbar-light .navbar-toggler {
    outline: none;
    border: none;
}

.nav-link:hover {
    color: #0095ff !important;
}

.navbar-toggler .navbar-toggle-icon {
    -webkit-transform: rotate(180deg);
    -ms-transform: rotate(180deg);
    -moz-transform: rotate(180deg);
    -o-transform: rotate(180deg);
    transform: rotate(180deg);
    -webkit-transition-duration: 0.7s;
    -moz-transition-duration: 0.7s;
    -o-transition-duration: 0.7s;
    transition-duration: 0.7s;
    width: 20px;
    height: 20px;
}

.navbar-collapse {
    width: 100%;
}

.navbar-toggler.collapsed .navbar-toggle-icon {
    -webkit-transform: rotate(0deg);
    -ms-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    transform: rotate(0deg);
    -webkit-transition-duration: 0.7s;
    -moz-transition-duration: 0.7s;
    -o-transition-duration: 0.7s;
    transition-duration: 0.7s;
    width: 20px;
    height: 20px;
}

.navbar-header {
    flex-wrap: nowrap;
    display: inline-flex;
    justify-content: space-between;
    align-items: center;
    width: 100%;
}

a.disabled {
    pointer-events: none;
}

.slide-in-right {
    opacity: 0;
    -webkit-animation: slideInRight 2s forwards;
    -moz-animation: slideInRight 2s forwards;
    -o-animation: slideInRight 2s forwards;
    animation: slideInRight 2s forwards;
    -webkit-transform: translate3d(0,0,0);
}

.slide-in-left-delayed {
    opacity: 0;
    -webkit-animation: slideInLeft 2s forwards;
    -moz-animation: slideInLeft 2s forwards;
    -o-animation: slideInLeft 2s forwards;
    animation: slideInLeft 2s forwards;
    -webkit-animation-delay: 1.8s;
    -moz-animation-delay: 1.8s;
    -o-animation-delay: 1.8s;
    animation-delay: 1.8s;
    -webkit-transform: translate3d(0,0,0);
}

.fade-in-delayed {
    opacity: 0;
    -webkit-animation: fadeIn 1s forwards;
    -moz-animation: fadeIn 1s forwards;
    -o-animation: fadeIn 1s forwards;
    animation: fadeIn 1s forwards;
    -webkit-animation-delay: 2.5s;
    -moz-animation-delay: 2.5s;
    -o-animation-delay: 2.5s;
    animation-delay: 2.5s;
    -webkit-transform: translate3d(0,0,0);
}

.home-background {
    height: 100vh;
}

.project-title {
    width: 100%;
    font-size: 16vw;
    color: black;
}

.home-background:before{
    z-index:-1;
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%, -50%);
    content: url({% asset homeBackground.jpg @path %});
    min-width: 0px;
    -webkit-animation: fadeInBackground 5s forwards;
    -moz-animation: fadeInBackground 5s forwards;
    -o-animation: fadeInBackground 5s forwards;
    animation: fadeInBackground 5s forwards;
    -webkit-animation-delay: 0.5s;
    -moz-animation-delay: 0.5s;
    -o-animation-delay: 0.5s;
    animation-delay: 0.5s;
    opacity: 0;
    backface-visibility: hidden;
}

.bg-transparent-white {
    background-color: rgba(251, 251, 251, 0.98);
}

.bg-transparent {
    background-color: rgba(251, 251, 251, 0.5) !important;
}

body {
    color: black;
}

.slow-transition-filters {
    max-width: 100vw;
    -webkit-transition: filter 1s ease-in-out;
    -moz-transition: filter 1s ease-in-out;
    -o-transition: filter 1s ease-in-out;
    transition: filter 1s ease-in-out;
    -webkit-filter: grayscale(0);
    filter: grayscale(0);
    backface-visibility: hidden;
}

.slow-transition-background {
    -webkit-transition: background-color 1s ease-in-out;
    -moz-transition: background-color 1s ease-in-out;
    -o-transition: background-color 1s ease-in-out;
    transition: background-color 1s ease-in-out;
    -webkit-filter: grayscale(0);
    filter: grayscale(0);
    backface-visibility: hidden;
}

.slow-transition-all {
    max-width: 100vw;
    -webkit-transition: all 1s ease-in-out;
    -moz-transition: all 1s ease-in-out;
    -o-transition: all 1s ease-in-out;
    transition: all 1s ease-in-out;
    -webkit-filter: grayscale(0);
    filter: grayscale(0);
    backface-visibility: hidden;
}

.greyscale {
    -webkit-filter: grayscale(1);
    filter: grayscale(1);
}

.shrink {
    max-width: 1340px !important;
}

ul#project-list li {
    display: inline;
    margin-left: 10px;
    margin-right: 10px;
}

.hidden {
    opacity: 0;
}

.zoom-blur {
    -webkit-transition: all 0.5s ease-in-out;
    -moz-transition: all 0.5s ease-in-out;
    -o-transition: all 0.5s ease-in-out;
    transition: all 0.5s ease-in-out;
    filter: blur(0px);
    opacity: 1;
}

.zoom-bounds {
    overflow: hidden;
    position: relative;
